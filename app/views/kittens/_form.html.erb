<%= form_for @kitten do |form| %>
  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>
 
  <p>
    <%= form.label :age %><br>
    <%= form.number_field :age %>
  </p>

  <p>
    <%= form.label :cuteness %><br>
    <%= form.range_field(:cuteness, in: 1..10) %>
  </p>

  <p>
    <%= form.label :softness %><br>
    <%= form.number_field :softness %>
  </p>
 
  <p>
    <%= form.submit "Meeeeow" %>
  </p>
<% end %>